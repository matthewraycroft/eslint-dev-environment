{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
      "setWithoutGet": true,
      "consistent-return": 1,
      "curly" : 1,
      "no-fallthrough": 1,
      "no-lone-blocks": 1
    }
}
